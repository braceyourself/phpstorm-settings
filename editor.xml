<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Victor Mono" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Medium" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.0" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>